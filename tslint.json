{
  "rules": {
    "no-any": false,
    "quotemark": {
      "options": "double"
    },
    "ordered-imports": false,
    "arrow-parens": true,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "singleline": "never",
        "esSpecCompliant": true
      }
    ]
  }
}
